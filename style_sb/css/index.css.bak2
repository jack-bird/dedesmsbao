#creat_plug #plug {width:1000px; margin:0 auto;}
#plug li {width:195px; height:50px; line-height:50px; float:left; text-align:left; }
#plug li a {color:#333; }
#plug li a:hover {color:#88B439; }


.web_home_link_box dd img {
    display: inline-block;
