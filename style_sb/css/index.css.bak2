.footer .service{width: 134px; margin-left: 14px;}
.footer .aboutus{width: 172px;}
.footer .support{width: 202px;}
.footer .business{width: 274px;}
.footer-title{position: relative;}
.footer-title span{position: absolute; background: url("../images/footer-nv.png"); width: 7px; height: 5px; bottom: 0; left: 21px;}
.footer-title h2{color: #ffffff; font-size: 16px; text-shadow: 0 1px 1px rgba(0, 0, 0, 0.45); line-height: 16px; padding: 34px 0 14px; border-bottom: 1px solid #fafafa;}
.footer-qcode{float: right; margin-top: 44px; text-align: center; color: #d4d4c0; font-size: 16px; line-height: 28px; text-shadow: 0 1px 1px rgba(0, 0, 0, 0.45); }
